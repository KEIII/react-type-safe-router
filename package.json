{
  "name": "type-safe-router",
  "version": "0.0.2",
  "private": true,
  "dependencies": {
    "@keiii/k-stream": "0.0.42",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/uri-templates": "^0.1.31",
    "@types/urijs": "^1.19.17",
    "fp-ts": "^2.11.5",
    "import-sort-style-module": "^6.0.0",
    "io-ts": "^2.2.16",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "0.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2",
    "uri-templates": "^0.2.0",
    "urijs": "^1.19.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": true,
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "auto"
  }
}
